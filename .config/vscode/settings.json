{
    "editor.tabSize": 2,
    "go.useLanguageServer": true,
    "go.autocompleteUnimportedPackages": true,
    "go.toolsEnvVars": {
        "GOBIN": "/home/xiashura/Projects/go/bin"
    },
    "gopls": {
        "completeUnimported": true
    },
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "search.quickOpen.includeHistory": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "variable.other.rust",
                "settings": {
                    "foreground": "#c651e5"
                }
            }
        ]
    },
    "[rust]": {
        "editor.defaultFormatter": "matklad.rust-analyzer"
    },
    "editor.snippetSuggestions": "top",
    "cSpell.language": "en,ru",
    "editor.lineNumbers": "relative",
    "workbench.editor.untitled.experimentalLanguageDetection": true,
    "editor.fontFamily": "FiraCode Nerd Font Mono",
    "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-j>": false,
        "<C-h>": false,
        "<C-b>": false,
        "<C-m>": false,
        "<C-w>": false,
        "<C-o>": false,
        "<C-t>": false
    },
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": [
        "#8FBCBB",
        "#434C5E"
    ],
    "vim.statusBarColors.insert": "#BF616A",
    "vim.statusBarColors.visual": "#B48EAD",
    "vim.statusBarColors.visualline": "#B48EAD",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#007ACC",
    "vim.statusBarColors.searchinprogressmode": "#007ACC",
    "vim.statusBarColors.easymotionmode": "#007ACC",
    "vim.statusBarColors.easymotioninputmode": "#007ACC",
    "vim.statusBarColors.surroundinputmode": "#007ACC",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "q",
                "q"
            ],
            "commands": [
                ":q!"
            ]
        },
        {
            "before": [
                "t",
                "k"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "t",
                "j"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "t",
                "n"
            ],
            "commands": [
                ":enew"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "workbench.colorCustomizations": {
        "statusBar.background": "#B48EAD",
        "statusBar.noFolderBackground": "#B48EAD",
        "statusBar.debuggingBackground": "#B48EAD",
        "statusBar.foreground": "#434C5E"
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "editor.multiCursorModifier": "ctrlCmd",
    "git.autofetch": true,
    "explorer.confirmDelete": false,
    "rust-analyzer.inlayHints.typeHints": true,
    "workbench.colorTheme": "GitHub Light",
    "editor.renderControlCharacters": true,
    "window.menuBarVisibility": "hidden",
    "breadcrumbs.enabled": false,
    "editor.minimap.enabled": false,
    "emmet.includeLanguages": {
        "html-eex": "html"
    },
    "cSpell.userWords": [
        "actix",
        "xiashura"
    ],
    "go.toolsManagement.autoUpdate": true,
    "workbench.preferredLightColorTheme": "GitHub Light",
    "workbench.preferredDarkColorTheme": "GitHub Dark",
    "sundial.sunrise": "04:00",
    "sundial.sunset": "16:00",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "editor.formatOnSave": true,
    "terminal.integrated.cursorStyle": "line",
    "window.zoomLevel": 1,
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path.linux": "/home/xi/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vscode-kubernetes.kubectl-path.linux": "/home/xi/.vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.minikube-path.linux": "/home/xi/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
    },
}